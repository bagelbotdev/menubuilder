{
  "manifest_version": 2,

  "name": "Bagelbot MenuItem Builder",
  "description": "Build bagelbot menuitems from network requets on balsam bagel's online order page",
  "version": "1.0.0",

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

  "permissions": [
    "https://www.toasttab.com/balsam-bagels/v3/*/*"
  ],

  "devtools_page": "devtools.html",
  "background": {
    "scripts": []
  }
}
